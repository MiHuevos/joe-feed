<html dir="rtl">
<head>
  <link rel="stylesheet" href="/open-sans/opensans.css" />
  <link rel="stylesheet" href="/cssreset.css" />
  <link rel="stylesheet" href="/dist/bundle.css" />
  <style>
    * {
      font-family: 'Open Sans Hebrew';
    }

    html, body {
      min-width: 100vw;
      margin: 0;
    }

    body {
      background-color: #eee;
    }

    a:link, a:active, a:visited {
      text-decoration: none;
    }

    a:hover {
      text-decoration: underline;
    }

    /*#main::before {*/

    #main > div {
      z-index: 1;
      margin: auto;
      margin-top: 1.1em;
      width: 90vw;
      max-width: 100vw !important;
    }
  </style>
</head>
<body>
  <div style="
    background-color: #274A6C;
    position: absolute;
    width: 100vw;
    height: 30vh;
    left: 0;
    top: 0;
    z-index: -1;
  ">
  </div>
  <div id="main"><%- prerenderedReact %></div>
  <script>
    window.initialState = <%- JSON.stringify(anotherKaki) %>;
  </script>
  <script src="/dist/<%- scriptName %>"></script>
</body>
</html>
